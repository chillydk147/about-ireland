 {\\r\\n    this.setState({\\r\\n      skip: (this.state.skip-this.state.limit)\\r\\n    });\\r\\n  }\\r\\n  setSkipNext() {\\r\\n    this.setState({\\r\\n      skip: (this.state.skip+this.state.limit)\\r\\n    });\\r\\n  }\\r\\n\\r\\n  render() {\\r\\n    const { data } = this.props;\\r\\n    const {skip, limit, county, mapListings, total} = this.state;\\r\\n    const to = (skip+limit) < total ? (skip+limit) : total;\\r\\n    const post = data.markdownRemark;\\r\\n    const counties = data.allMarkdownRemark.distinct;\\r\\n    const directoryListings = data.allMarkdownRemark.edges;\\r\\n\\r\\n    const { slug, currentPage, numPages } = this.props.pageContext\\r\\n\\r\\n    return (\\r\\n      <Layout headerImage={post.frontmatter.category}>\\r\\n        <div>\\r\\n          <h1>{post.frontmatter.category} in Ireland</h1>  \\r\\n          <select onChange={this.filterCounty} className={directoryListingStyles.directoryListingSelect}>\\r\\n            <option value=\\\"\\\">All Ireland</option>\\r\\n            {counties.map((county, key)=>{\\r\\n              return (\\r\\n                <option value={county} key={key}>{county}</option>\\r\\n              )\\r\\n            })}\\r\\n          </select>\\r\\n          <Map directoryListings={mapListings} skip={skip} limit={limit} countyFilter={county}/>     \\r\\n          <div className={directoryListingStyles.directoryListingNavButtons}>\\r\\n            {!this.isFirst() && (\\r\\n              <span onClick={this.setSkipPrev}>PREV</span>\\r\\n            )}\\r\\n            {!this.isLast() && (\\r\\n              <span onClick={this.setSkipNext}>NEXT</span>\\r\\n            )}\\r\\n          </div>     \\r\\n          <div className={directoryListingStyles.directoryListingShowing}>\\r\\n            Showing <b>{(skip+1)}</b> to <b>{to}</b> of <b>{total}</b>\\r\\n          </div>     \\r\\n          <DirectoryListings directoryListings={directoryListings} skip={skip} limit={limit} countyFilter={county}/>\\r\\n      